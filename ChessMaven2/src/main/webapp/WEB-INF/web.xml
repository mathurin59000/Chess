<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Auth</servlet-name>
  	<display-name>Auth</display-name>
  	<description></description>
  	<servlet-class>org.example.servlets.Auth</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<display-name>Register</display-name>
  	<description></description>
  	<servlet-class>org.example.servlets.Register</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WebSocketChatServlet</servlet-name>
  	<display-name>WebSocketChatServlet</display-name>
  	<description></description>
  	<servlet-class>org.example.servlets.WebSocketChatServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Auth</servlet-name>
  	<url-pattern>/auth</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WebSocketChatServlet</servlet-name>
  	<url-pattern>/WebSocketChatServlet</url-pattern>
  </servlet-mapping>
</web-app>
